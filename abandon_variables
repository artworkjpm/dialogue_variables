<#global ABAN_RETURNDATE=dayadd(.now,17)?string("dd/MM")></#if><#global ABAN_RIID_ = ABANDON_BASKET.ABAN_RIID_><#if ALL_CONTENT_IS_SET == 'N'>    	<#if SEGMENTATION_1_COL.SEGMENTATION_IATA == ABAN_ARRIVALAIRPORTCODE>
