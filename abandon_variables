        <#global ABAN_DATA_ID=data.ABAN_DATA_ID>
        <#if DV_DEBUG_ABANDON == "Y">
            <h1>Abandon basket data found           </h1>
        	ABAN_DATA_ID: ${data.ABAN_DATA_ID?c}<br>
 			ABAN_BASKET_URL: ${data.ABAN_BASKET_URL}<br>
        </#if>
