<#-- CAMPAIGN_TEMPLATE SUP TABLE -->
<#assign MODULE_IS_SET = 'N'>
<#global SEGMENTED_MODULES = []>
<#data CAMPAIGN_TEMPLATE as CAMPAIGN_TEMPLATE>
<#filter TEMPLATE_NAME=TEMPLATE>
<#fields TEMPLATE_MODULE>
<#if !SEGMENTED_MODULES?seq_contains(CAMPAIGN_TEMPLATE.TEMPLATE_MODULE?upper_case)><#global SEGMENTED_MODULES = SEGMENTED_MODULES + [CAMPAIGN_TEMPLATE.TEMPLATE_MODULE?upper_case]>
</#if>
</#data>
