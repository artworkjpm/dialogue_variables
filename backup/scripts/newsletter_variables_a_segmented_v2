            <#if SEGMENT_ARTICLE_IMG_IS_SET == 'N' || SEGMENTED_IATA_ARTICLE_IS_SET == 'N'><#global SEGMENTED_IMAGE_ARTICLE = ARTICLE_FALLBACK_IMG><#global SEGMENTED_HEADING_ARTICLE = ARTICLE_FALLBACK_HEADING><#global SEGMENTED_INTRO_ARTICLE = ARTICLE_FALLBACK_INTRO><#global SEGMENTED_PARAGRAPH_ARTICLE = ARTICLE_FALLBACK_PARAGRAPH><#global SEGMENTED_URL_ARTICLE = ARTICLE_FALLBACK_URL>
