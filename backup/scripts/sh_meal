                	<#-- test variables end-->
                <#assign MEAL_AVAILABLE_YN = "Y">
                	<#-- test variables start-->
                	<#if campaign.name == 'DEBUG_E_SH_2DA'><span style="color:red">MEAL_AVAILABLE_YN: ${MEAL_AVAILABLE_YN}<br></span>
                	</#if>
                <#-- test variables end-->
                <#assign MEAL_ON_SH_IS_SET = 1>
                <#assign MEAL_AVAILABLE_IS_SET = 1>
           <#else><#-- city pair is not found or releasedate are not reached, set the variable to N -->
               <#assign MEAL_AVAILABLE_YN = "N">
           </#if>
       </#if><#-- if MEAL_ON_SH_IS_SET is not 0, it will continue the loop to check for next record -->
   <#else><#-- city pair have NULL value for releasedate, set both variables to N -->
       <#assign MEAL_AVAILABLE_YN = "N">
       <#assign HAS_MEAL = "N">
   </#if>
   <#if HAS_MEAL == 'Y'><#assign MEAL_AVAILABLE_YN = "N"><#-- If meal is bought on at least one of the legs, we will not sell meal on the other legs where meal are not ordered - The customer might have made a decision to only book meal for certain legs -->
   </#if>   
</#list>


<#-- If the citypair is not in the list above, assign meal available to N -->
<#if MEAL_AVAILABLE_YN?isnull><#assign MEAL_AVAILABLE_YN = "N"></#if>
                    
 <#global MEAL_AVAILABLE = MEAL_AVAILABLE_YN>                  
<#if campaign.name == 'DEBUG_E_SH_2DA'><span style="color:green">MEAL_AVAILABLE_YN: ${MEAL_AVAILABLE_YN}<br><strong>${LEG_ORIGIN_IATA}-${LEG_DESTINATION_IATA}:</strong><br><#if TEST_CITY_PAIR_FOUND == 1>Meal is available from: ${LEG_RELEASE_DATE}<br></#if> LEG_DEPARTURE_TIME: ${SQL_GLOBAL.LEG_DEPARTURE_TIME?date?string("dd.MM.yyyy")}<br>LEG_RELEASE_DATE: ${LEG_RELEASE_DATE}<br>LEG_ORDER_FROM_DATE: ${LEG_ORDER_FROM_DATE}<br>Today's date: ${.now?date?string("dd.MM.yyyy")}<br>MEAL_AVAILABLE: ${MEAL_AVAILABLE}<br>HAS_MEAL: ${HAS_MEAL}</span><hr></#if>


